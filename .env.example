# API 配置
VITE_API_BASE_URL=http://localhost:3000/api

# OpenAI 配置
VITE_OPENAI_API_KEY=your_openai_api_key

# MongoDB 配置
MONGODB_URI=mongodb://localhost:27017/jubensha

# JWT 配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# 文件上传配置
UPLOAD_DIR=uploads
