# 服务器配置
PORT=3000
NODE_ENV=development

# MongoDB 配置
MONGODB_URI=mongodb://localhost:27017/story_character_manager

# JWT 配置
JWT_SECRET=your_jwt_secret
JWT_EXPIRES_IN=7d

# OpenAI 配置
OPENAI_API_KEY=your_openai_api_key

# 文件上传配置
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880 # 5MB

# 日志配置
LOG_LEVEL=debug
LOG_FORMAT=dev 